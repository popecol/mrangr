  bibtype = "manual",
  title = "mrangr: Mechanistic Metacommunity Simulator",
  author = c(
    person("Author1_name", "Author1_surname"),
    person("Author2_name", "Author2_surname")),
  year = "2025",
  note = "R package version 1.0.1"
)
